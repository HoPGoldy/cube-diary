{
  "name": "cube-diary",
  "version": "0.1.4",
  "main": "index.js",
  "repository": "git@github.com:HoPGoldy/cube-diary.git",
  "author": "HoPGoldy <hpgless@outlook.com>",
  "bin": {
    "cube-diary": "dist/cli/index.js"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "build": "npm run lint && npm run server:build && npm run client:build",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts --ext .tsx",
    "server:dev": "nodemon -e ts --watch src --ignore src/client --exec ts-node -r tsconfig-paths/register --project tsconfig.server.json --files src",
    "server:build": "tsc --project tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "client:dev": "vite --host",
    "client:build": "tsc && vite build"
  },
  "dependencies": {
    "@bytemd/plugin-gfm": "^1.20.2",
    "@bytemd/plugin-highlight": "^1.21.0",
    "@bytemd/react": "^1.20.2",
    "@reduxjs/toolkit": "^1.9.1",
    "antd": "^5.3.2",
    "axios": "^1.3.3",
    "commander": "^10.0.0",
    "copy-to-clipboard": "^3.3.2",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.4",
    "fs-extra": "^10.1.0",
    "joi": "^17.6.0",
    "js-cookie": "^3.0.1",
    "json5": "^2.2.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.4.2",
    "koa": "^2.13.4",
    "koa-body": "^5.0.0",
    "koa-jwt": "^4.0.3",
    "koa-logger": "^3.2.1",
    "koa-router": "^10.1.1",
    "koa-send": "^5.0.1",
    "koa-static": "^5.0.0",
    "koa2-connect-history-api-fallback": "^0.1.3",
    "lodash": "^4.17.21",
    "nanoid": "3.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-markdown": "^8.0.5",
    "react-query": "^3.39.3",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.2",
    "react-sortablejs": "^6.1.4",
    "react-transition-group": "^4.4.5",
    "remark-gfm": "^3.0.1",
